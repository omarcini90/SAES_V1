<script type="text/javascript">
    $(window).on("load", function () {
        $(".loader1").fadeOut("slow");
    });
    function loader_push() {
        $(".loader1").fadeIn("slow");
    }
    function loader_stop() {
        $(".loader1").fadeOut("slow");
    }
</script>
